{
	Properties
	{
		_Cutoff( "Mask Clip Value", Float ) = 0.5
		_Tile_A("Tile_A", 2D) = "white" {}
		_Tile_N("Tile_N", 2D) = "bump" {}
		_Tile_M("Tile_M", 2D) = "white" {}
		_Tile_O("Tile_O", 2D) = "white" {}
